#include <iostream> 
#include <conio.h> 
using namespace std;

int x = 50; //variabel eksternal 

void main() 
{ 
	double x; // definisi variabel lokal 
	x = 5.678901234; // variabel lokal yg diberi nilai 
	cout<<x<<" " <<::x<<endl; 
	::x= 77; //variabel eksternalyg diberi nilai 
	cout<<x<< " "<<::x<<endl; 
}
